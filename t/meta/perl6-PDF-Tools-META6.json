        "PDF::Storage::IndObj" : "lib/PDF/Storage/IndObj.pm",
        "PDF::Storage::Input" : "lib/PDF/Storage/Input.pm",
        "PDF::Storage::Input::IOH" : "lib/PDF/Storage/Input/IOH.pm",
